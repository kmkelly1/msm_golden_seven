<div class="page-header">
    <h1>Director #<%= @director.id %> </h1>
</div>

<dl class="dl-horizontal">
    
    
    <dt>DOB</dt>
    <dd><%=@director.dob %></dd>
    
    <dt>Name</dt>
    <dd><%=@director.name %></dd>
    
    <dt>Bio</dt>
    <dd><%=@director.bio %></dd>
    
    <dt>Image</dt>
    <dd><%= @director.image_url %></dd>
</dl>

<p><a href="/delete_director/<%= @director.id %>" class="btn btn-danger">Delete director</a></p>

<p><a href="/directors/<%= @director.id %>/edit_form" class="btn btn-primary">Edit director</a></p>